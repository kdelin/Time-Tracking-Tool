// Define the Packagepackage classes.view.grid {	import flash.utils.getDefinitionByName;		// Import Necessary Classes		// Define the Class	internal dynamic final class GridData extends Array {				// Create a Varianble for the Arguments		internal const arg:Object = { columnLabels: [ ], rowData: [ [ ] ], visibleRows:3 };		// Save a Reference to the Number of Rows		public var totalRows:int = 0;		// Save a Reference to the Number of Visible Rows		public var visibleRows:int = 3;				// Instantiate the Class		public function GridData ( obj:Object = null ) : void {			// Limit Enumeration			for ( var i:String in this ) this.setPropertyIsEnumerable ( i, false );			// Initialize Arguments			for ( i in obj ) if ( i in arg ) arg [ i ] = obj [ i ];			// Save a Reference to the Number of Columns			length = arg.columnLabels.length;			// Save a Reference to the Number of Rows			totalRows = arg.rowData.length;			// Save a Reference to the Number of Rows			visibleRows = arg.visibleRows;			// Loop through Column Data			for ( var n:int = length; n--; ) {				// Specify the Index Number				obj.index = n;				// Create an Object for Column Data				this [ n ] = new GridColumnData ( obj );			}		}	}}