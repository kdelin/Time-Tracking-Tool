// Define the Packagepackage classes.view.timefield {		import flash.events.Event;		public class TimeFieldEvent extends Event {		/// set up custom events		public static const DONE							:String = "Closing TimeField";				// set up vars		public var time										:Number;						public function TimeFieldEvent(type:String, bubbles:Boolean, cancelable:Boolean, content:String):void {			super(type, bubbles, cancelable);			this.time = time;		}				override public function clone():Event {			return new TimeFieldEvent(this.type, this.bubbles, this.cancelable, this.content);		}		override	 public function toString():String {			return formatToString("TimeFieldEvent","type","bubbles","cancelable","eventPhase","content");		}	}			}