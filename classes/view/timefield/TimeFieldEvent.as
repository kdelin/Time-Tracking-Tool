// Define the Packagepackage classes.view.timefield {		import flash.events.Event;		public class TimeFieldEvent extends Event {		public static const DONE							:String = "User closed text field";				// set up vars		public var content									:String;						public function TimeFieldEvent(type:String, bubbles:Boolean, cancelable:Boolean, content:String):void {			super(type, bubbles, cancelable);			this.content = content;		}				override public function clone():Event {			return new TimeFieldEvent(this.type, this.bubbles, this.cancelable, this.content);		}		override	 public function toString():String {			return formatToString("TimeFieldEvent","type","bubbles","cancelable","eventPhase","content");		}	}			}